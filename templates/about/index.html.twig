{% extends 'base.html.twig' %}

{% block title %}{{ page_title }}{% endblock %}

{% block body %}
    <h1>{{ info.title }}</h1>
    <p>{{ info.description }}</p>

    <h2>Notre mission</h2>
    <p>{{ info.mission }}</p>

    <h2>Notre vision</h2>
    <p>{{ info.vision }}</p>

    <h2>Nos valeurs</h2>
    <ul>
        {% for value in info.values %}
            <li>{{ value }}</li>
        {% endfor %}
    </ul>
{% endblock %}
